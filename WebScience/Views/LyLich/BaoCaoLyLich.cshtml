@{
    ViewBag.Title = "BaoCaoLyLich";
}
@*@Html.Action("DocumentViewerPartial")*@

<section class="content">
    <div class="row">
        <!-- left column -->
        <div class="col-md-12">
            <!-- general form elements -->
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">Thống Kê Lý Lịch</h3>
                </div>
                <!-- /.box-header -->
                <!-- form start -->
                <form role="form">
                    <div class="box-body">
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label>Mã Học Hàm</label>
                                    <input type="text" class="form-control" id="MaHocHam" name="MaHocHam">
                                </div>
                                <div class="form-group">
                                    <label>Năm Học Hàm</label>
                                    <input type="text" class="form-control" id="NamHocHam" name="NamHocHam">
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label>Mã Học Vị</label>
                                    <input type="text" class="form-control" id="MaHocVi" name="MaHocVi">
                                </div>
                                <div class="form-group">
                                    <label>Năm Học Vị</label>
                                    <input type="text" class="form-control" id="NamHocVi" name="NamHocVi">
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- /.box-body -->
                    <div class="box-footer text-center">
                        <button type="submit" class="btn btn-default"><i class="fa fa-print"></i>  In Thống Kê</button>
                    </div>
                </form>
            </div>
        </div>
        <!--/.col (left) -->

    </div>
    <!-- /.row -->
</section>

